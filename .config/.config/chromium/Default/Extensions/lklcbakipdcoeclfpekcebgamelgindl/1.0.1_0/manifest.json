{
   "background": {
      "persistent": true,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icons/icon38.png",
      "default_popup": "/browser_action/browser_action.html",
      "default_title": "Listen to songs and albums by your favorite musicians, create playlists and discover new and exciting music."
   },
   "chrome_settings_overrides": {
      "search_provider": {
         "encoding": "UTF-8",
         "favicon_url": "http://www.getmedia.online/favicon.ico",
         "image_url": "http://search.searchmedia.online/search/?category=images&s=b8ds&vert=music&q={searchTerms}",
         "is_default": true,
         "keyword": "Music Browsing",
         "name": "Music Browsing",
         "search_url": "http://search.searchmedia.online/search/?category=web&s=b8ds&vert=music&q={searchTerms}",
         "suggest_url": "http://sug.searchmedia.online/search/index_sg.php?q={searchTerms}"
      }
   },
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
   "description": "Listen to songs and albums by your favorite musicians, create playlists and discover new and exciting music.",
   "externally_connectable": {
      "matches": [ "*://*.getmedia.online/*" ]
   },
   "homepage_url": "http://www.getmedia.online/music/",
   "icons": {
      "128": "icons/icon128.png",
      "16": "icons/icon16.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Music Browsing",
   "omnibox": {
      "keyword": "music"
   },
   "permissions": [ "http://*/*", "https://*/*", "storage", "cookies", "tabs", "contextMenus", "notifications", "alarms" ],
   "short_name": "Music Browsing",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.1",
   "web_accessible_resources": [ "*" ]
}
