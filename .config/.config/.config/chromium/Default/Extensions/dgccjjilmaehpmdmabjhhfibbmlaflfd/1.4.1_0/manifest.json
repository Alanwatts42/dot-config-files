{
   "author": "Kelvin Hung",
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "glasses24.png",
      "default_title": "Magic Reader"
   },
   "content_scripts": [ {
      "js": [ "jquery.js", "content_script.js", "eye-test.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
   "description": "Adjust the font size and line spacing of the main content of a webpage automatically based on your preference.",
   "icons": {
      "128": "glasses128.png",
      "16": "glasses24.png",
      "48": "glasses48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Magic Reader",
   "options_page": "options.html",
   "permissions": [ "tabs", "activeTab", "storage", "contextMenus" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.4.1"
}
