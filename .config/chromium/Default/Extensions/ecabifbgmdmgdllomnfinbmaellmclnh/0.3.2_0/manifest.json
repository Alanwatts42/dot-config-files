{
   "author": "Richard Neomy",
   "background": {
      "persist": false,
      "scripts": [ "config.js", "common.js" ]
   },
   "commands": {
      "toggle-reader-view": {
         "description": "Toggle the Reader View"
      }
   },
   "description": "strips away clutter like buttons, background images, and changes the page's text size, contrast and layout for better readability",
   "homepage_url": "https://add0n.com/chrome-reader-view.html",
   "icons": {
      "128": "data/icons/128.png",
      "16": "data/icons/16.png",
      "256": "data/icons/256.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 2,
   "name": "Reader View",
   "optional_permissions": [ "tabs", "*://*/*" ],
   "options_ui": {
      "chrome_style": true,
      "page": "data/options/index.html"
   },
   "page_action": {
      "default_icon": {
         "16": "data/icons/16.png",
         "32": "data/icons/32.png",
         "48": "data/icons/48.png",
         "64": "data/icons/64.png"
      }
   },
   "permissions": [ "storage", "activeTab", "contextMenus", "notifications", "declarativeContent", "chrome://favicon/" ],
   "short_name": "reader-view",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.3.2"
}
